    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/app/cmd/web",
            "env": {
                "db-source": "tcp(:3306)/picinch",
                "db-password": "test-only",
                "admin-name": "admin@example.com",
                "admin-password": "admin-test-only",
                "domains": "",
                "email-host": "smtp.mailtrap.io",
                "email-user": "your-username",
                "email-password": "your-password",
                "geo-block": "KP,RU",
                "sender": "noreply@example.com",
                "reply-to": "",
                "video-package": "jrottenberg/ffmpeg:4-scratch",
                "video-types": ".mp4,.mov",
                "test": "../../picinch-test/srv/picinch"
               },
            "args": [],
            "cwd": "${workspaceFolder}/app",
        }
    ]
}